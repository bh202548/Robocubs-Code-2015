public class  ElevatortoOneTote extends Command {

	boolean finished = false;
	
    public ElevatortoOneTote() {
        // Use requires() here to declare subsystem dependencies
        // eg. requires(chassis);

        // BEGIN AUTOGENERATED CODE, SOURCE=ROBOTBUILDER ID=REQUIRES
        requires(Robot.elevator);

    // END AUTOGENERATED CODE, SOURCE=ROBOTBUILDER ID=REQUIRES
    }

    // Called just before this Command runs the first time
    protected void initialize() {
    }

    // Called repeatedly when this Command is scheduled to run
    protected void execute() {
    	finished = false;
    	int polarity = 1;
    	if(RobotMap.elevatorHeight < 4){
    		polarity = -1;
    	}
    	if(!RobotMap.elevatorOneToteLimitSwitch.get()){
    		RobotMap.elevatorElevatorMotorL.set(polarity * 0.5);
    		RobotMap.elevatorElevatorMotorR.set(polarity * 0.5);
    	}else{
    		finished = true;
    	}if(RobotMap.elevatorHeight == 4){
    		finished = true;
    	}
    	if(RobotMap.elevatorTopLimitSwitch.get() && RobotMap.elevatorHeight != 7){
    		finished = true;
    	}
    	if(RobotMap.elevatorBottomLimitSwitch.get() && RobotMap.elevatorHeight != 0){
    		finished = true;
    	}
    	if(Math.abs(Robot.oi.getElevatorJoystick().getY()) > 0.15){
    		finished = true;
    	}
    }

    // Make this return true when this Command no longer needs to run execute()
    protected boolean isFinished() {
        return finished;
    }

    // Called once after isFinished returns true
    protected void end() {
    	RobotMap.elevatorElevatorMotorL.set(0);
		RobotMap.elevatorElevatorMotorR.set(0);
		RobotMap.elevatorHeight = 4;
    }

    // Called when another command which requires one or more of the same
    // subsystems is scheduled to run
    protected void interrupted() {
    	finished = true;
    }
}
